pragma solidity ^0.5.0;

import "./Token.sol";

import "./ISunswapV2Router01.sol";


contract  Vendor {
    using SafeMath for uint256;
    TRC20  usdt;
    Token  mes;

    ISunswapV2Router01 router;
    address b;
    address a;

    uint256 fees = 12;
    address private owner;


    constructor(TRC20 _usdt,Token _mes,ISunswapV2Router01 _router,address _a,address _b) public  {
        usdt = _usdt;
        mes = _mes;
        router = _router;
        owner = msg.sender;
        a = _a;
        b = _b;

    }

    event BuyToken(address buyer, uint256 amountOfUSDT, uint256 amountOfTokens);

    event SellToken(address seller, uint256 amountOfTokens, uint256 amountOfUSDT);


    function getTokenPrice() private view returns (uint[] memory amount1){

        address[] memory  path;

        path[0] = address(a);
        path[1] = address(b);

        amount1 = router.getAmountsOut(uint256(1000000),path);

        return amount1;
    }


    function  buyToken(uint256 amount) public payable returns (bool) {



        require(amount > 0, "Specify an amount of token greater than zero");

        uint256 vendorBalance = mes.balanceOf(address(this));
        require(vendorBalance > amount, "Insufficient MES balance in the contract");

        uint256 my_balance = usdt.balanceOf(msg.sender);

        require(my_balance > amount, "Insufficient USDT balance");

        (bool sent) = usdt.transferFrom(msg.sender, address(this), amount);

        require(sent, "Failed to transfer tokens from user to vendor");

        //fees
        uint256 feesAmount = amount.mul(fees).div(100);

        uint256 arrivalAmount = amount.sub(feesAmount);

        (bool sent1) = mes.transfer(msg.sender, arrivalAmount);

        require(sent1, "Failed to transfer tokens");

        emit BuyToken(msg.sender, msg.value, amount);

        return true;
    }

    function  sellToken(uint256 amount) public payable returns (bool) {

        require(amount > 0, "Specify an amount of token greater than zero");

        uint256 my_balance = mes.balanceOf(msg.sender);

        require(my_balance > amount, "Insufficient MES balance");

        uint256 tokenBalance = usdt.balanceOf(address(this));

        require(tokenBalance > amount, "Insufficient USDT balance in the contract");

        (bool sent) = mes.transferFrom(msg.sender, address(this), amount);
        require(sent, "Failed to transfer Toekn");

        (bool sent1) = usdt.transfer(msg.sender,amount);
        require(sent1, "Failed to transfer USDT");

        emit SellToken(msg.sender, msg.value, amount);

        return true;
    }

    function withdrawUsdt(uint256 amount,address recipient) public returns (bool)  {

        require(msg.sender == owner, "Failed to send user balance back to the owner1");

        uint256 ownerBalance = usdt.balanceOf(address(this));

        require(ownerBalance > amount, "Owner has not balance to withdraw");

        (bool sent) = mes.transferFrom(msg.sender, recipient, amount);
        require(sent, "Failed to send user balance back to the owner2");

        return true;
    }

    function withdrawMes(uint256 amount,address recipient) public returns (bool)  {

        require(msg.sender == owner, "Failed to send user balance back to the owner1");

        uint256 ownerBalance = mes.balanceOf(address(this));

        require(ownerBalance > amount, "Owner has not balance to withdraw");

        (bool sent) = mes.transferFrom(msg.sender, recipient, amount);
        require(sent, "Failed to send user balance back to the owner2");

        return true;
    }



}