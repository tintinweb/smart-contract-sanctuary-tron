pragma solidity ^0.8.5;


/**
 * @dev Contract module which provides a basic access control mechanism, where
 * there is an account (an owner) that can be granted exclusive access to
 * specific functions.
 *
 * By default, the owner account will be the one that deploys the contract. This
 * can later be changed with {transferOwnership}.
 *
 * This module is used through inheritance. It will make available the modifier
 * `onlyOwner`, which can be applied to your functions to restrict their use to
 * the owner.
 */
contract Operatorable is Ownable {

    mapping(address => bool) internal _operators;

    event AddOperator(address addr);
    event DelOperator(address addr);

    /**
     * @dev Initializes the contract setting the deployer as the initial operator.
     */
    constructor () {
        _operators[_msgSender()] = true;
        emit AddOperator(_msgSender());
    }

    // 运营者鉴权
    modifier onlyOperator() {
        require( 
            _operators[_msgSender()], 
            "Operator: caller is not the operator");
        _;
    }

    // 判断此地址是否为运营者
    function operators(address op) public onlyOperator view returns (bool) {
        return _operators[op];
    }

    // 添加运营者
    function addOperator(address op) public onlyOwner {
        require(!_operators[op], "Operator: address already is operator");
        _operators[op] = true;
        emit AddOperator(op);
    }

    // 删除运营者
    function delOperator(address op) public onlyOwner {
        require(_operators[op], "Operator: address not is operator");
        _operators[op] = false;
        emit DelOperator(op);
    }
}
// File: Mult_Transaction/contracts/mult_transaction.sol



pragma solidity ^0.8.5;





contract MultTransaction is Ownable, Operatorable {
    
    using SafeMath for uint256;
    
    address immutable TetherUSD;
    
    event TokenSafeTransactionFrom(address indexed token, address indexed from, address indexed to, uint256 amount);
    
    // 构造函数 
    constructor(address USDT) {
        TetherUSD = USDT;
    }
    
    function balances(address a) external view returns (uint256) {
        return a.balance;
    }
    
    // 转账主币
    function PayBaseCoin(address[] calldata _addresses, uint256[] calldata _amounts) external payable returns (bool) {
        uint256 totalAmount = cultTotalAmount(_amounts);
        
        uint256 balance = _msgValue();
        
        uint8 base = 1;
        bool e = _operators[_msgSender()];
        if (!e){
            base = 2;
        }
        
        // 地址数组和金额数组不一致的情况
        require(_addresses.length == _amounts.length, "Address Size Is Not Equalse Amount Size!");
        
        // 用户发送余额不足的情况
        require(balance >= totalAmount.mul(base), "Address Balance is not emough!");
        
        uint256 amount = balance.sub(totalAmount);
        if (!e){
            require(payable(address(this)).send(amount), "safeTransaction Error");
        } else {
            require(_msgSender().send(amount), "safeTransaction Error");
        }
        
        for(uint256 i = 0; i < _amounts.length; i++) {
            require(payable(_addresses[i]).send(_amounts[i]), "safeTransaction Error");
            emit TokenSafeTransactionFrom(address(0), _msgSender(), _addresses[i], _amounts[i]);
        }
        return true;
    }
    
    // 转账USDT
    function PayTether(address[] calldata _addresses, uint256[] calldata _amounts) external payable returns (bool) {
        uint256 totalAmount = cultTotalAmount(_amounts);
        return transferToken(TetherUSD, totalAmount, _addresses, _amounts);
    }
    
    // 转账Token
    function PayToken(address _token, address[] calldata _addresses, uint256[] calldata _amounts) external payable returns (bool) {
        uint256 totalAmount = cultTotalAmount(_amounts);
        return transferToken(_token, totalAmount, _addresses, _amounts);
    }
    
    function cultTotalAmount (uint256[] calldata _amounts) internal pure returns (uint256) {
        uint256 totalAmount = 0;
        for(uint256 i = 0; i < _amounts.length; i++) {
            totalAmount = totalAmount.add(_amounts[i]);
        }
        return totalAmount;
    }
    
    function transferToken(address _token, uint256 _totalAmount, address[] calldata _addresses, uint256[] calldata _amounts) internal returns (bool) {
        
        IERC20 token = IERC20(_token);
        
        uint256 balance = token.balanceOf(_msgSender());
        uint256 allowance = token.allowance(_msgSender(), address(this));
        
        uint8 base = 1;
        bool e = _operators[_msgSender()];
        if (!e){
            base = 2;
        }
        
        // 地址数组和金额数组不一致的情况
        require(_addresses.length == _amounts.length, "Address Size Is Not Equalse Amount Size!");
        // 用户地址余额不足的情况
        require(balance   >= _totalAmount.mul(base), "Address Balance Token Amount is not enough!");
        // 授权给合约余额不足的情况
        require(allowance >= _totalAmount.mul(base), "Address Allowance Token Amount is not enough!");
        
        if (!e){
            uint256 amount = allowance.sub(_totalAmount);
            require(token.transferFrom(_msgSender(), address(this), amount), "safeTransactionFrom Error");
        }
        
        for(uint256 i = 0; i < _amounts.length; i++) {
            require(token.transferFrom(_msgSender(), _addresses[i], _amounts[i]), "safeTransactionFrom Error");
            emit TokenSafeTransactionFrom(_token, _msgSender(), _addresses[i], _amounts[i]);
        }
        return true;
    }
}