pragma solidity ^0.7.0;




contract InvisibleTrxFriends is ERC721, Ownable {
    using SafeMath for uint256;

    struct UserStruct {
        bool exist;
        uint mintedInvi;
        uint addressPoolIndex;
        uint level;
        uint reward;
    }

    string public INVI_PROVENANCE = "";

    string public baseExtension = ".json";

    uint256 public startingIndexBlock;

    uint256 public startingIndex;




    mapping (address => UserStruct) public users;
    mapping (uint256 => address) public addressPool;
    uint public addressPoolCount = 1;
    uint public level1Count = 0;
    uint public level2Count = 0;

    //// Random index assignment
    uint internal nonce = 0;
    uint[MAX_INVI] internal indices;

    uint256 public constant inviPrice = 500000000; // 500 

    uint public constant maxInviPurchase = 20;

    uint256 public constant MAX_INVI = 5000;

    bool public saleIsActive = false;

    uint256 public REVEAL_TIMESTAMP;

    address payable internal saleReceiver1;
    address payable internal saleReceiver2;

    constructor(string memory name, string memory symbol, uint256 saleStart, address payable _saleReceiver1 , address payable _saleReceiver2) ERC721(name, symbol) {
        REVEAL_TIMESTAMP = saleStart + (86400 * 9);
        saleReceiver1 = _saleReceiver1;
        saleReceiver2 = _saleReceiver2;
    }

    function withdraw() public onlyOwner {
        uint balance = address(this).balance;
        msg.sender.transfer(balance);
    }

    
    function setRevealTimestamp(uint256 revealTimeStamp) public onlyOwner {
        REVEAL_TIMESTAMP = revealTimeStamp;
    } 

    
    function setProvenanceHash(string memory provenanceHash) public onlyOwner {
        INVI_PROVENANCE = provenanceHash;
    }

    function setBaseURI(string memory baseURI) public onlyOwner {
        _setBaseURI(baseURI);
    }

    
    function flipSaleState() public onlyOwner {
        saleIsActive = !saleIsActive;
    }

    function randomIndex() internal returns (uint) {
        uint totalSize = MAX_INVI - totalSupply();
        uint index = uint(keccak256(abi.encodePacked(nonce, msg.sender, block.difficulty, block.timestamp))) % totalSize;
        uint value = 0;
        if (indices[index] != 0) {
            value = indices[index];
        } else {
            value = index;
        }

        // Move last value to selected position
        if (indices[totalSize - 1] == 0) {
            // Array position not initialized, so use position
            indices[index] = totalSize - 1;
        } else {
            // Array position holds a value so use that
            indices[index] = indices[totalSize - 1];
        }
        nonce++;
        // Don't allow a zero index, start counting at 1
        return value.add(1);
    }

    
    function mintInvi(uint numberOfTokens) public payable {
        require(saleIsActive, "Sale must be active to mint Invisible");
        require(numberOfTokens <= maxInviPurchase, "Can only mint 20 Invisibles at a time");
        require(totalSupply().add(numberOfTokens) <= MAX_INVI, "Purchase would exceed max supply of Invisibles");
        require(inviPrice.mul(numberOfTokens) <= msg.value, "Tron  put 500 value sent is not correct");
        
        for(uint i = 0; i < numberOfTokens; i++) {
            uint mintIndex = randomIndex();
            if (totalSupply() < MAX_INVI) {
                _safeMint(msg.sender, mintIndex);
            }
        }

        uint totalPrice = inviPrice.mul(numberOfTokens);

        
        if (startingIndexBlock == 0 && (totalSupply() == MAX_INVI || block.timestamp >= REVEAL_TIMESTAMP)) {
            startingIndexBlock = block.number;
        }

        uint saleReceiver1Amount = 0;
        uint saleReceiver2Amount = 0;

        if(users[msg.sender].exist) {
            users[msg.sender].mintedInvi+=numberOfTokens;

            if(users[msg.sender].mintedInvi >= 500 && users[msg.sender].level < 2){

                users[msg.sender].level = 2;
                level2Count ++;
                level1Count --;
            }
            else if(users[msg.sender].mintedInvi >= 400 && users[msg.sender].level < 1){
                level1Count++;
                users[msg.sender].level = 1;
                addressPool[addressPoolCount] = msg.sender;
                users[msg.sender].addressPoolIndex = addressPoolCount;
                addressPoolCount++;
            }
        } else {

            UserStruct memory userStruct;

            userStruct = UserStruct({
                exist: true,
                mintedInvi: numberOfTokens,
                addressPoolIndex: 0,
                level: 0,
                reward: 0
            });

            users[msg.sender] = userStruct;
        }


        uint saleReceivers = 90;
        uint level1Reward = 0;
        uint level2Reward= 0;


        if(level1Count > 0) {
            level1Reward = ((totalPrice * 3)/100) / level1Count;
        }else{
            saleReceivers += 3;
        }


        if(level2Count > 0){
            level2Reward = ((totalPrice * 7)/100) / level2Count;
        } else {
            saleReceivers += 7;
        }

        for(uint i = 1; i <= addressPoolCount ; i++){
            address userPoolAddress = addressPool[i];

            if(users[userPoolAddress].level == 2 && level2Count > 0){
                users[userPoolAddress].reward += level2Reward;
            }else if(users[userPoolAddress].level == 1 && level1Count > 0){
                users[userPoolAddress].reward += level1Reward;
            }
        }

        uint saleReceiver1Reward = (saleReceivers * 10) / 100;
        uint saleReceiver2Reward = (saleReceivers * 90) / 100;

        saleReceiver1Amount += (totalPrice * saleReceiver1Reward) / 100;
        saleReceiver2Amount += (totalPrice * saleReceiver2Reward) / 100;

        saleReceiver1.transfer(saleReceiver1Amount);
        saleReceiver2.transfer(saleReceiver2Amount);
    }

    /**
     * Withdraw collected rewards
     */
    function withdrawReward() external returns(uint){
        require(users[msg.sender].exist == true, "User does not exist");
        require(users[msg.sender].reward > 0, "No rewards");


        msg.sender.transfer(users[msg.sender].reward);
        users[msg.sender].reward = 0;
    }
}