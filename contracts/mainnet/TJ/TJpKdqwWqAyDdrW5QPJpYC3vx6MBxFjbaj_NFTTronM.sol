contract ZDManager is ZDOwner {
    address[] private vManagers;

    modifier onlyManager() {
      bool bOk = false;
      if(owner == msg.sender)
        bOk = true;
      else{
        for(uint i = 0; i < vManagers.length; i++){
          if(msg.sender == vManagers[i]){
            bOk = true;
            break;
          }
        }
      }
      require(bOk,"error 5005");
      _;
    }

    function IsMngAddr(address saddr) public view returns (bool){
      if(owner == saddr)
        return true;
      else{
        for(uint i = 0; i < vManagers.length; i++){
          if(saddr == vManagers[i]){
            return true;
          }
        }
      }
      return false;
    }

    function AddMngAddr( address mngAddr) public onlyOwner{
        vManagers.push(mngAddr);
    }
    function RemoveMngAddr( address mngAddr) public onlyOwner{
        for(uint32 i = 0; i < vManagers.length; i++){
            if(vManagers[i] == mngAddr){
                for(uint32 n = i; n < vManagers.length-1; n++){
                    vManagers[n]    = vManagers[n+1];
                }
                break;
            }
        }
        vManagers.pop();
    }

}

