interface Data {
    function isOld(address _account) external view returns(bool);
}

contract OldUser is OwnableOperatorRole {
    Data data;
    mapping (address => bool) public flag;

    constructor(Data _data) {
        data = _data;
    }

    function isOld(address _account) external view returns(bool) {
        return flag[_account] ? false : data.isOld(_account);
    }

    function setNew(address _account) external onlyOperator {
        flag[_account] = true;
    }

    function setOld(address _account) external onlyOperator {
        flag[_account] = false;
    }
}