// SPDX-License-Identifier: MIT
pragma solidity  ^0.8.0;

import "./ERC20.sol";
import "./Address.sol";


contract TOC is ERC20{

    using Address for address;

    address public owner;//权限管理
    address public mintAddress        = 0x2B4F827eA3836a911834a8d7Ac2705CC5a5abD72;//接收代币的账户
    address public revenueAddress     = 0xFe1E938ca3361dD8f13629b7E080C7ee9eeA8bF4;//5%平台营收账号
    address public stimulateAddress   = 0xBE9e5a487647369660BE4600D9F8b9BE6c8C4D43;//5%平台激励账号
    address public weightAddress      = 0x204C1A972d520ec68324F28709d63B520ea28c92;//加权地址10%
    address public constant blackHole = 0x0000000000000000000000000000000000000001;//黑洞地址10%

    mapping (address => address) public userWhiteList;//加入的用户不收手续费
    mapping (address => bool) public LPList; //加入的LP地址有手续费

    constructor() ERC20("toc.token","TOC"){
        owner = _msgSender();
        _mint(mintAddress, 38888 *  10 ** 18);
    }

    //权限
    modifier onlyOwner() {
        require(owner == _msgSender(), "Ownable: caller is not the owner");
        _;
    }
    function setOwner(address _owner) public onlyOwner {
        owner = _owner;
    }
    //5%平台营收账号
    function setRevenueAddress(address account) public onlyOwner {
        revenueAddress = account;
    }
    //5%平台激励账号
    function setStimulateAddress(address account) public onlyOwner {
        stimulateAddress = account;
    }
    //加权地址10%
    function setWeightAddress(address account) public onlyOwner {
        weightAddress = account;
    }
    //设置白名单
    function setUserWhiteList(address LPAddress, address account) public onlyOwner {
        userWhiteList[LPAddress] = account;
        LPList[LPAddress] = true;
    }
//    //发币
//    function TRCMint(address account, uint256 amount) public onlyOwner {
//        _mint(account, amount);
//    }
    //重写erc20方法
    function transfer(address recipient, uint256 amount) public virtual override returns (bool) {
        //msg.sender 是调用此方法的合约地址
        if(LPList[msg.sender]) {
            if(userWhiteList[msg.sender] == recipient) {
                super.transfer(recipient, amount);
                return true;
            }else {
                require(balanceOf(msg.sender) >= amount, "ERC20: transfer amount exceeds balance");
                uint256 gainAmount = amount * 85 / 100;//接收账户 85%
                uint256 resAmount = amount * 10 / 100;//手续费15%-10%
                uint256 serviceAmount = amount * 5 / 100;//手续费15%-5%
                //买toc
                _transfer(_msgSender(),  recipient, gainAmount);
                _transfer(_msgSender(),  blackHole, resAmount);//销毁
                _transfer(_msgSender(),  stimulateAddress, serviceAmount);
                return true;
            }
        }else {
            super.transfer(recipient, amount);
            return true;
        }


    }
    //重写erc20方法
    function transferFrom(address sender, address recipient, uint256 amount) public override returns (bool) {
        //msg.sender 是调用此方法的合约地址
        if(LPList[msg.sender]) {
            if(userWhiteList[msg.sender] == sender || userWhiteList[msg.sender] == recipient) {
                super.transferFrom(sender, recipient, amount);
                return true;
            }else {
                require(balanceOf(sender) >= amount, "ERC20: transfer amount exceeds balance");
                uint256 gainAmount = amount * 85 / 100;//接收账户 85%
                uint256 resAmount = amount * 10 / 100;//手续费15%-10%
                uint256 serviceAmount = amount * 5 / 100;//手续费15%-5%
                //买
                if(sender == msg.sender) {
                    _transfer(sender, recipient, gainAmount);
                    _transfer(sender, blackHole, resAmount);//销毁
                    _transfer(sender, stimulateAddress, serviceAmount);
                }
                //卖
                if(recipient == msg.sender) {
                    _transfer(sender, recipient, gainAmount);
                    _transfer(sender, weightAddress , resAmount);//15%的手续费其中 10%加到池子里加权分红（按 lp池子里的算力）
                    _transfer(sender, revenueAddress, serviceAmount);
                }

                uint256 currentAllowance = allowance(sender, _msgSender());
                require(currentAllowance >= amount, "ERC20: transfer amount exceeds allowance");
            unchecked {
                _approve(sender, _msgSender(), currentAllowance - amount);
            }
                return true;
            }
        }else {
            super.transferFrom(sender, recipient, amount);
            return true;
        }
    }
}