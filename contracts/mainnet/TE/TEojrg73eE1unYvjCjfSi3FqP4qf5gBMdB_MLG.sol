        address tokenRecverAddress,
        address feeAddress
    ) payable ERC20("MLG", "MLG") {
        ISunswapV2Router02 sunswapV2Router = ISunswapV2Router02(sunswapRouter);
        address sunswapV2Pair = ISunswapV2Factory(sunswapV2Router.factory())
            .createPair(address(this), sunswapV2Router.WETH());

        _feeAddress = feeAddress;
        _sunswapV2Router = sunswapV2Router;
        _sunswapV2Pair = sunswapV2Pair;

        _dividendTracker = new TokenDividendTracker(_sunswapV2Pair);

        _isExcludedFromFees[tokenRecverAddress] = true;
        _isExcludedFromFees[address(this)] = true;
        _isExcludedFromFees[address(_dividendTracker)] = true;

        _isDividendExempt[address(this)] = true;
        _isDividendExempt[address(0)] = true;
        _isDividendExempt[address(_dividendTracker)] = true;

        _marketSupply = _marketSupply.add(_baseInitAmount);
        _mint(tokenRecverAddress, _baseInitAmount);
    }

    receive() external payable {}

    function isExcludedFromFees(address account) public view returns (bool) {
        return _isExcludedFromFees[account];
    }

    function setExcludedFromFees(address account) public onlyOwner {
        _isDividendExempt[account] = true;
    }

    function setSwapTokensAtAmount(uint256 amount) public onlyOwner {
        _swapTokensAtAmount = amount;
    }

    function setMinPeriod(uint256 number) public onlyOwner {
        _minPeriod = number;
    }

    function resetLPRewardLastSendTime() public onlyOwner {
        _dividendTracker.resetLPRewardLastSendTime();
    }

    function updateDistributorGas(uint256 newValue) public onlyOwner {
        require(
            newValue >= 100000 && newValue <= 500000,
            "distributorGas must be between 200,000 and 500,000"
        );
        require(
            newValue != _distributorGas,
            "Cannot update distributorGas to same value"
        );
        _distributorGas = newValue;
    }

    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal override {
        require(from != address(0), "ERC20: transfer from the zero address");
        require(to != address(0), "ERC20: transfer to the zero address");
        if (amount == 0) {
            super._transfer(from, to, 0);
            return;
        }

        uint256 contractTokenBalance = balanceOf(address(this));
        bool canSwap = contractTokenBalance >= _swapTokensAtAmount;
        if (
            canSwap &&
            !_swapping &&
            from != _sunswapV2Pair &&
            from != owner() &&
            to != owner()
        ) {
            _swapping = true;
            if (_amountLpRewardFee > 0) {
                swapLPRewardToken(_amountLpRewardFee);
                _amountLpRewardFee = 0;
            }
            _swapping = false;
        }

        (string memory tradeType, bool takeFee) = _getTradeTypeAndTakeFee(
            from,
            to
        );

        if (_swapping) {
            takeFee = false;
        }

        _tokenTransfer(from, to, amount, tradeType, takeFee);

        if (_fromAddress == address(0)) _fromAddress = from;
        if (_toAddress == address(0)) _toAddress = to;
        if (!_isDividendExempt[_fromAddress] && _fromAddress != _sunswapV2Pair)
            try _dividendTracker.setShare(_fromAddress) {} catch {}
        if (!_isDividendExempt[_toAddress] && _toAddress != _sunswapV2Pair)
            try _dividendTracker.setShare(_toAddress) {} catch {}
        _fromAddress = from;
        _toAddress = to;

        if (
            !_swapping &&
            from != owner() &&
            to != owner() &&
            from != address(this) &&
            _dividendTracker.LPRewardLastSendTime().add(_minPeriod) <=
            block.timestamp
        ) {
            try _dividendTracker.process(_distributorGas) {} catch {}
        }
    }

    function totalSupply() public view override returns (uint256) {
        return _totalSupply;
    }

    function _calculateSaleFee(uint256 _amount) private view returns (uint256) {
        return _amount.mul(_total_saleFee).div(_totalFeeRatio);
    }

    function swapLPRewardToken(uint256 tokenAmount) private {
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = _sunswapV2Router.WETH();
        _approve(address(this), address(_sunswapV2Router), tokenAmount);
        _sunswapV2Router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0,
            path,
            address(_dividendTracker),
            block.timestamp
        );
    }

    function _tokenTransfer(
        address _sender,
        address _recipient,
        uint256 _amount,
        string memory _tradeType,
        bool _takeFee
    ) private {
        uint256 feeAmount = 0;
        uint256 toAmount = _amount;
        if (isEqual(_tradeType, "sale") || isEqual(_tradeType, "contract")) {
            if (_takeFee) {
                feeAmount = _calculateSaleFee(_amount);
                if (feeAmount > 0) {
                    _takeSaleFee(_sender, feeAmount);
                }
            }
        }
        toAmount = toAmount.sub(feeAmount);
        super._transfer(_sender, _recipient, toAmount);
    }

    function _takeSaleFee(address _sender, uint256 _amount) private {
        uint256 saleFeeOneAmount = _amount.mul(_saleFee).div(_total_saleFee);
        uint256 salelpRewardFeeAmount = _amount.mul(_salelpRewardFee).div(
            _total_saleFee
        );

        super._transfer(_sender, _feeAddress, saleFeeOneAmount);

        _amountLpRewardFee += salelpRewardFeeAmount;
        super._transfer(_sender, address(this), salelpRewardFeeAmount);
    }

    function _getTradeTypeAndTakeFee(address _sender, address _recipient)
        private
        view
        returns (string memory, bool)
    {
        bool takeFee = true;
        string memory tradeType = "";
        if (!Address.isContract(_sender) && !Address.isContract(_recipient)) {
            takeFee = false;
            tradeType = "transfer";
        } else if (
            Address.isContract(_sender) && !Address.isContract(_recipient)
        ) {
            tradeType = "buy";
        } else if (
            !Address.isContract(_sender) && Address.isContract(_recipient)
        ) {
            tradeType = "sale";
        } else if (
            Address.isContract(_sender) && Address.isContract(_recipient)
        ) {
            tradeType = "contract";
        }
        if (_isExcludedFromFees[_sender] || _isExcludedFromFees[_recipient]) {
            takeFee = false; //excluded fee
        }
        return (tradeType, takeFee);
    }

    function isEqual(string memory a, string memory b)
        private
        pure
        returns (bool)
    {
        bytes memory aa = bytes(a);
        bytes memory bb = bytes(b);
        if (aa.length != bb.length) return false;
        for (uint256 i = 0; i < aa.length; i++) {
            if (aa[i] != bb[i]) return false;
        }
        return true;
    }

    function mint(uint256 amount) public onlyOwner {
        require(
            _marketSupply.add(amount) <= _totalSupply,
            "can't not Mint More!"
        );
        _mint(_miningContractAddress, amount);
        _marketSupply = _marketSupply.add(amount);
    }

    function miningMint(uint256 amount) public onlyMining {
        require(
            _marketSupply.add(amount) <= _totalSupply,
            "can't not Mint More!"
        );
        _mint(_miningContractAddress, amount);
        _marketSupply = _marketSupply.add(amount);
    }

    function setMiningContractAddress(address mining) public onlyOwner {
        _miningContractAddress = mining;
    }
}