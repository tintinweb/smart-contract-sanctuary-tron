    uint256 public _baseInitAmount = 79 * 10**18;

    uint256 public _marketSupply = 0;
    uint256 public _totalSupply = 679 * 10**18;
    uint256 public _swapTokensAtAmount = 1 * 10**6;

    address public _miningContractAddress;

    bool _swapping;

    ISunswapV2Router02 _sunswapV2Router;
    address public _sunswapV2Pair;

    uint256 public _saleFee = 200;
    uint256 public _salelpRewardFee = 600;

    uint256 public _total_saleFee = _saleFee + _salelpRewardFee;

    mapping(address => bool) private _isExcludedFromFees;

    mapping(address => bool) _isDividendExempt;

    TokenDividendTracker _dividendTracker;
    uint256 public _amountLpRewardFee;

    address _fromAddress;
    address _toAddress;

    uint256 public _minPeriod = 86400;
    uint256 _distributorGas = 200000;

    uint256 public _totalFeeRatio = 10000;
