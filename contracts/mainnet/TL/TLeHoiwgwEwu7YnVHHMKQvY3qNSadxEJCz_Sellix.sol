        // construction, since the code is only stored at the end of the
        // constructor execution.

        uint256 size;
        // solhint-disable-next-line no-inline-assembly
        assembly {
            size := extcodesize(account)
        }
        return size > 0;
    }
}

contract Sellix {
    using SafeMath for uint256;
    using SafeTRC20 for ITRC20;

    address public owner;
    uint256 public tokenSendFee; // in wei
    uint256 public trxSendFee; // in wei

    constructor() {
        owner = msg.sender;
    }

    modifier onlyOwner() {
        require(msg.sender == owner);
        _;
    }

    function balance() public view returns (uint256) {
        return address(this).balance;
    }

    function balanceToken(ITRC20 tokenAddr) public view returns (uint256) {
        return tokenAddr.balanceOf(address(this));
    }

    function deposit() public payable returns (bool) {
        return true;
    }

    function withdrawTrx(address payable addr, uint256 amount)
        public
        onlyOwner
        returns (bool success)
    {
        addr.transfer(amount);
        return true;
    }

    function withdrawToken(
        ITRC20 tokenAddr,
        address _to,
        uint256 _amount
    ) public onlyOwner returns (bool success) {
        tokenAddr.safeTransfer(_to, _amount);
        return true;
    }

    function multiPayoutToken(
        ITRC20 tokenAddr,
        address[] calldata addresses,
        uint256[] calldata amounts
    ) public onlyOwner returns (bool success) {
        uint256 total = 0;

        for (uint8 i = 0; i < amounts.length; i++) {
            total = total.add(amounts[i]);
        }

        // check if user has enough balance
        require(
            total <= tokenAddr.balanceOf(address(this)),
            "Not enough token balance"
        );

        for (uint8 j = 0; j < addresses.length; j++) {
            tokenAddr.safeTransfer(addresses[j], amounts[j]);
        }

        return true;
    }

    function multiPayoutTrx(
        address payable[] calldata addresses,
        uint256[] calldata amounts
    ) public onlyOwner returns (bool success) {
        uint256 total = 0;

        for (uint8 i = 0; i < amounts.length; i++) {
            total = total.add(amounts[i]);
        }

        // ensure that there is enough to complete the transaction
        uint256 requiredAmount = total.add(trxSendFee);
        require(
            address(this).balance >= (requiredAmount),
            "Not enough balance"
        );

        for (uint8 j = 0; j < addresses.length; j++) {
            addresses[j].transfer(amounts[j]);
        }

        return true;
    }

    function setTokenFee(uint256 _tokenSendFee)
        public
        onlyOwner
        returns (bool success)
    {
        tokenSendFee = _tokenSendFee;
        return true;
    }

    function setTrxFee(uint256 _trxSendFee)
        public
        onlyOwner
        returns (bool success)
    {
        trxSendFee = _trxSendFee;
        return true;
    }

    function contractAddress() public view onlyOwner returns (address) {
        return address(this);
    }

    function destroy(address payable _to) public onlyOwner {
        selfdestruct(_to);
    }
}