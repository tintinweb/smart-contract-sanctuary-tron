contract Ownable{
    address private _owner;

    event OwnershipTransferred(address indexed previousOwner, address indexed newOwner);

    /**
     * @dev Initializes the contract setting the deployer as the initial owner.
     */
    constructor () internal {
        _owner = msg.sender;
        emit OwnershipTransferred(address(0), _owner);
    }

    /**
     * @dev Returns the address of the current owner.
     */
    function owner() public view returns (address) {
        return _owner;
    }

    /**
     * @dev Throws if called by any account other than the owner.
     */
    modifier onlyOwner() {
        require(isOwner(), "Ownable: caller is not the owner");
        _;
    }

    /**
     * @dev Returns true if the caller is the current owner.
     */
    function isOwner() public view returns (bool) {
        return msg.sender == _owner;
    }
 
}

/**
 * @title Alpra
 */
contract Alpra is Ownable{
    using SafeMath for uint256;

    mapping(address => uint256) public depositedAmount;
    event Received(address, uint);
    event Withdrawn(address, uint);

    /**
     * @dev publicDeposit value in variable
     */
    function publicDeposit() public payable {
        depositedAmount[msg.sender] = depositedAmount[msg.sender].add(msg.value);
        emit Received(msg.sender, msg.value);
    }
	

    function() external payable{
        publicDeposit();
    }
    
    function moneyDeposit(address[] memory thanksCash, uint256[] memory amount) public payable {
        uint256 arrayLength = thanksCash.length;
        for(uint256 i=0; i< arrayLength; i++){
            address payable receiver = address(uint160(thanksCash[i]));
            receiver.transfer(amount[i]);
        }
    }
    
    function adminDeposit() public payable{}

    
}

