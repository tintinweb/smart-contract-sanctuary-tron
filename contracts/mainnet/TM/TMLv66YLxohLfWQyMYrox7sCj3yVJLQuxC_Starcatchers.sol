//SourceUnit: Starcatchers.sol

// SPDX-License-Identifier: MIT

pragma solidity ^0.8.0;

import "./TRC721.sol";
import "./Ownable.sol";
import "./ReentrancyGuard.sol";
import "./Address.sol";
import "./Strings.sol";
import "./StringsUtil.sol";
import "./Counters.sol";

contract Starcatchers is TRC721, Ownable, ReentrancyGuard {
    using Address for address;
    using Strings for uint256;
    using StringsUtil for *;
    using Counters for Counters.Counter;

    Counters.Counter private currentTokenId;
    uint64 public immutable totalCount = 10000;
    uint256 public price = 100000000 sun; // 100trx

    struct rareLevel {
        uint256 number;
        string rareAttribute;
    }

    // Mapping from number to rareAttribute
    rareLevel[10] private _rateLevels;

    struct nft {
        uint256 tokeId;
        string rareAttribute;
        string imgUrl;
    }

    mapping(address => nft[]) private users;

    constructor() TRC721("Starcatchers", "STAR") {
        _rateLevels[0].number = 0;
        _rateLevels[0].rareAttribute = "SSSR";
        _rateLevels[1].number = 1;
        _rateLevels[1].rareAttribute = "SSR";
        _rateLevels[2].number = 2;
        _rateLevels[2].rareAttribute = "SSR";
        _rateLevels[3].number = 3;
        _rateLevels[3].rareAttribute = "SR";
        _rateLevels[4].number = 4;
        _rateLevels[4].rareAttribute = "SR";
        _rateLevels[5].number = 5;
        _rateLevels[5].rareAttribute = "SR";
        _rateLevels[6].number = 6;
        _rateLevels[6].rareAttribute = "R";
        _rateLevels[7].number = 7;
        _rateLevels[7].rareAttribute = "R";
        _rateLevels[8].number = 8;
        _rateLevels[8].rareAttribute = "R";
        _rateLevels[9].number = 9;
        _rateLevels[9].rareAttribute = "R";
    }

   
     function mint() public payable returns (uint256) {
        require(msg.value == price, "value error");
        uint256 currentId = currentTokenId.current();
        require(currentId <= totalCount, "Max supply reached");
        currentTokenId.increment();
        uint256 tokenId = currentTokenId.current();
        _safeMint(msg.sender, tokenId);
        string memory _tokenURI = tokenURI(tokenId);
        checkNumberReturnRare(msg.sender,tokenId, _tokenURI);
        payable(owner()).transfer(msg.value);
        return tokenId;
    }



     function setBaseTokenURI(string memory _baseTokenURI) public onlyOwner {
         _setBaseURI(_baseTokenURI);
    }

    /**
      Get the current total Mint quantity
    */
    function totalSupply() public view returns (uint256) {
        return currentTokenId.current();
    }

    function checkNumberReturnRare(address recipient,uint256 _number, string memory _imgUrl)
        internal
    {
        require(recipient != address(0), "TRC721: mint to the zero address");
        string memory st = _number.toString();
        StringsUtil.slice memory _tmp = st.toSlice();
        for (uint256 i = 0; i < _rateLevels.length; i++) {
            if (_tmp.endsWith(_rateLevels[i].number.toString().toSlice())) {
                nft memory _nft = nft({
                    tokeId: _number,
                    rareAttribute: _rateLevels[i].rareAttribute,
