pragma solidity ^0.5.0;

import "./ERC20.sol";
import "./ERC20Detailed.sol";
import "./Ownable.sol";
import "./IJustswapExchange.sol";
import "./IJustswapFactory.sol";
import "./IStake.sol";

contract Token is ERC20, ERC20Detailed {
    IStake public stake;

    address payable public LPAddress;

    mapping(address => bool) public whiteList;

    uint public feePercent = 1000;
    
    uint public sellDestroy = 30;
    uint public buyFee1 = 10;
    uint public buyFee2 = 10;
    uint public buyFee3 = 80;


    address public feeTo1;
    address public feeTo2;
    address public feeTo3;
    
    bool public init;

    IJustswapFactory public factory;

    constructor(string memory _name, string memory _symbol, uint8 _decimals) public ERC20Detailed(_name, _symbol, _decimals) {
        _mint(msg.sender, 2400000000 * (10 ** uint256(decimals())));

        // doInit();
    }

    function doInit() public {
        if(init == false) {
            // dev TGcTZWZ9wtXizfYduij9ZqVbQ6JcuEUPEy
            // factory = IJustswapFactory(address(0x41119bda6eba8d9fb2aae75bca468b045a86fd5fea));
            // feeTo = msg.sender;

            //sha TSbVLhvS1KwpLeJjortTcjKqZjYoU6VwVi
            // factory = IJustswapFactory(address(0x41b65ea045c2226a606c64405a2cc46e49b69e29ec));
            // feeTo1 = msg.sender;
            // feeTo2 = msg.sender;
            // feeTo3 = msg.sender;

            //prod
            factory = IJustswapFactory(address(0x41eed9e56a5cddaa15ef0c42984884a8afcf1bdebb));
            // 运营钱包 TReh6N3oJ5buZkBkLvvD3fBuPWEJEK7pZj 市场 TDyXqhWXoMaepuLfMjxbZExf76Z9fNtefq 基金会钱包 TGNzVzWpjPVbqgKiYPPBbg84JhXrNGwGTQ
            feeTo1 = address(0x41ac018f0fef3249eb0e909d3b2264ee05a1093f49);
            feeTo2 = address(0x412bf03a946d9d022547ca020c5db7512d53b4b4b8);
            feeTo3 = address(0x4146504d670406ac2e8529f2c513255e32ca25a723);

            LPAddress = factory.getExchange(address(this));
            if(LPAddress == address(0)) {
                LPAddress = factory.createExchange(address(this));
            }
            init = true;
        }
    }

    function _transfer(address sender, address recipient, uint256 amount) internal {
        
        if( recipient == LPAddress ){
            uint amount_destroy = amount.mul(sellDestroy).div(feePercent);
            super._burn(sender, amount_destroy);

            uint256 fees1 = amount.mul(buyFee1).div(feePercent);
            super._transfer(sender, feeTo1, fees1);

            uint256 fees2 = amount.mul(buyFee2).div(feePercent);
            super._transfer(sender, feeTo2, fees2);

            uint256 fees3 = amount.mul(buyFee3).div(feePercent);
            super._transfer(sender, feeTo3, fees3);

            amount = amount.sub(amount_destroy).sub(fees1).sub(fees2).sub(fees3);
        }

        super._transfer(sender, recipient, amount);
    }
}