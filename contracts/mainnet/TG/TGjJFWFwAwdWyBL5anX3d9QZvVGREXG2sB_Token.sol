pragma solidity 0.5.10;

import "./Token.sol";

contract Proxy {

  function name(Token proxied) public view returns (string memory) {
      return proxied.name();
  }
  
  function balanceOf(Token proxied, address account) public view returns (uint256) {
      return proxied.balanceOf(account);
  }

  function allowance(Token proxied, address owner, address spender) public view returns (uint256) {
      return proxied.allowance(owner, spender);
  }

  function decimals(Token proxied) public view returns (uint8) {
    return proxied.decimals();
  }
}


