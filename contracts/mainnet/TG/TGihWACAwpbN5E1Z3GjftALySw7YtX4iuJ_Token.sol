      if(_isExcludedFromFee[_msgSender()] || _isExcludedFromFee[recipient]){
           _transfer(_msgSender(), recipient, amount);
        }else{
          if(burnEnabled){
              uint256 burnAmount = amount.mul(burnFee).div(100);
              uint256 trueAmount = amount.sub(burnAmount);
              _transfer(_msgSender(), recipient, trueAmount);
              _burn(_msgSender(), burnAmount);
