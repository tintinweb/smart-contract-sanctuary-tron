// File: contracts/SappySeals.sol

pragma solidity ^0.8.0;

contract SappySeals is TRC721, Ownable {
    uint256 public totalCount = 10000; //bruhTotal
    uint256 public price = 100 * 1e6; // 1 TRX

    bool public started = true;

    mapping(uint256 => string) public tokenRaity;

    struct TokenINfo {
        uint256 tokenId;
        string url;
        string raity;
    }

    constructor() TRC721("SappySeals", "sappySealToken") {}

    modifier mintEnabled() {
        require(started, "not started");
        _;
    }

    function setBaseURI(string memory baseURI_) public onlyOwner {
        _setBaseURI(baseURI_);
    }

    function changePrice(uint256 _newPrice) public onlyOwner {
        price = _newPrice;
    }

    function setNormalStart(bool _start) public onlyOwner {
        started = _start;
    }

    function mint() public payable mintEnabled {
        require(totalSupply() + 1 <= totalCount, "too much");
        require(msg.value == price, "value error");
        payable(owner()).transfer(msg.value);
        uint256 tokenId = totalSupply() + 1;
        _mint(_msgSender(), tokenId);
        setTokenRaity(tokenId);
    }

    function setTokenRaity(uint256 _tokenID) internal {
        // msg.sender
        uint256 endNum = _tokenID % 10;
        string memory raity;
        if (endNum == 0) {
            raity = "SSSR";
        } else if (endNum == 1 || endNum == 2) {
            raity = "SSR";
        } else if (endNum == 3 || endNum == 4 || endNum == 5) {
            raity = "SR";
        } else {
            raity = "R";
        }
        tokenRaity[_tokenID] = raity;
    }

    function getUserNftList()
        public
        view
        returns (TokenINfo[] memory userInfo)
    {
        uint256 tokenBalance = balanceOf(_msgSender());

        userInfo = new TokenINfo[](tokenBalance);
        if (tokenBalance > 0) {
            for (uint8 i = 0; i < tokenBalance; i++) {
                uint256 tokenId = tokenOfOwnerByIndex(_msgSender(), i);

                userInfo[i] = TokenINfo(
                    tokenId,
                    tokenURI(tokenId),
                    tokenRaity[tokenId]
                );
            }
        }
    }
}