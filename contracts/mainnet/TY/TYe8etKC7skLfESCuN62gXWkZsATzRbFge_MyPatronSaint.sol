    uint256 public preProvideSingle = 10 * (10 ** 6) * 30;

    address private _admin;
    address private _pendingAdmin;

    mapping(address => uint256) private _balances;

    mapping(address => mapping(address => uint256)) private _allowances;

    mapping(uint256 => ValidPeriod) private _emission;

    uint256 private _totalMint;
    uint256 private _totalSupply;

    string private _name;
    string private _symbol;
    uint8 private _decimals;
    address private _numenAddress;

    struct ValidPeriod {
        uint256 startTimestamp;
        uint256 endTimestamp;
        uint256 lastClaimTimestamp;
    }

    event NewAdmin(address oldAdmin, address newAdmin);
    event InitialNumen(uint256 tokenIndex);

    modifier onlyAdmin {
        require(msg.sender == _admin, "Only admin can call this function");
        _;
    }

    /**
     * @dev Sets the values for {name} and {symbol}, initializes {decimals} with
     * a default value of 18. Also initializes {emissionStart}
     *
