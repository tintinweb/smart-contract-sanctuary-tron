    function decimals() public view returns (uint8) {
        return _decimals;
    }

    function admin() public view returns (address) {
        return _admin;
    }

    function pendingAdmin() public view returns (address) {
        return _pendingAdmin;
    }

    function numenAddress() public view returns (address) {
        return _numenAddress;
    }

    /**
     * @dev See {ITRC20-totalSupply}.
     */
    function totalSupply() public view override returns (uint256) {
        return _totalSupply;
    }

    /**
     * @dev See {ITRC20-balanceOf}.
     */
    function balanceOf(address account) public view override returns (uint256) {
        return _balances[account];
    }

    /**
     * @dev initial Numen Token emission.
     */
    function initialEmission(uint256 tokenIndex) external returns (bool){
        require(msg.sender == _numenAddress, "Only Numen contract can call this function");
        uint256 currentTimestamp = block.timestamp;
        if (_emission[tokenIndex].endTimestamp != 0) {
            require(currentTimestamp >= _emission[tokenIndex].endTimestamp, "No expiration time");
        }
        _emission[tokenIndex] = ValidPeriod({startTimestamp : currentTimestamp, endTimestamp : currentTimestamp + SECONDS_IN_A_DAY * INITIAL_EMISSION_DAYS, lastClaimTimestamp : 0});

        emit InitialNumen(tokenIndex);
        return true;
    }

    /**
     * @dev Return Numen token expirationTimestamp
     * - `tokenIndex` Numen token index validity end timestamp
     */
    function validityEndTimestamp(uint256 tokenIndex) public view returns (uint256) {
        return _emission[tokenIndex].endTimestamp;
    }

    /**
     * @dev Accumulated MPS tokens for a Numen token index.
     */
    function accumulated(uint256 tokenIndex) public view returns (uint256) {
        uint256 currentTimestamp = block.timestamp;
        require(currentTimestamp > _emission[tokenIndex].startTimestamp, "Emission has not started yet");
        require(ITRC721(_numenAddress).ownerOf(tokenIndex) != address(0), "Owner cannot be 0 address");

        uint256 lastClaimed = uint256(_emission[tokenIndex].lastClaimTimestamp) != 0 ? uint256(_emission[tokenIndex].lastClaimTimestamp) : _emission[tokenIndex].startTimestamp;

        // Sanity check if last claim was on or after emission end
        if (lastClaimed >= _emission[tokenIndex].endTimestamp) return 0;

        // Getting the min value of both
        uint256 accumulationPeriod = currentTimestamp < _emission[tokenIndex].endTimestamp ? currentTimestamp : _emission[tokenIndex].endTimestamp;

        uint256 totalAccumulated = accumulationPeriod.sub(lastClaimed).mul(emissionPerDay).div(SECONDS_IN_A_DAY);

        return totalAccumulated;
    }

    /**
     * @dev Claim mints MPS for a Numen token index.
     */
    function claim(uint256 tokenIndex) public returns (uint256) {
        address tokenOwner = ITRC721(_numenAddress).ownerOf(tokenIndex);
        require(tokenOwner != address(0), "The token is not found");

        if (msg.sender != _numenAddress) {
            require(tokenOwner == msg.sender, "Sender is not the owner");
        }

        uint256 claimAmount = accumulated(tokenIndex);
        require(claimAmount != 0, "No accumulated MPS");
        _emission[tokenIndex].lastClaimTimestamp = block.timestamp;

        _mint(tokenOwner, claimAmount);
        return claimAmount;
    }

    /**
     * @dev it is only callable once.
