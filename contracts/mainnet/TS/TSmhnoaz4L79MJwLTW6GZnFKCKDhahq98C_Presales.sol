interface Deployed { 
    function tuinum(address to) external view returns (uint256);
    function tui(address to) external view returns (address);
}

contract Presales is Ownable {
    using SafeMath for uint256;
    address public usdt;
    address public futc;
    address public fff;
    address public ysz;
    uint public ratio;
    mapping(address => uint256) public tuinum2;
    mapping(address => address) public tui2;
    uint256 public one;
    uint256 public two;
    uint256 public three;
    uint256 public zong = 100000000;
    constructor() public {
        ratio = 3;
        one = 20;
        two = 15;
        three = 10;
    }
    function presales(uint _cx, address spender) external {
        address mtuiold = Deployed(fff).tui(msg.sender);
        address ttuiold = Deployed(fff).tui(spender);
        uint256 mtuinumold = Deployed(fff).tuinum(msg.sender);
        uint256 ttuinumold = Deployed(fff).tuinum(spender);
        require(tui2[msg.sender] == address(0) && mtuiold == address(0), "no presales");
        require(mtuiold == address(0), "no presales");
        require(_cx >= zong, "must be 100");
        uint dh = _cx * 100 * ratio; 
        if(tui2[msg.sender] == address(0) && mtuiold == address(0)){
            tui2[msg.sender] = spender;
            tuinum2[spender] = tuinum2[spender].add(1);
        }

        if(tuinum2[spender]+ttuinumold >= 100){
            IERC20(futc).transfer(spender , (dh * one)/100);
        }else if(tuinum2[spender]+ttuinumold >= 30){
            IERC20(futc).transfer(spender , (dh * two)/100);
        }else if(tuinum2[spender]+ttuinumold >= 10){
            IERC20(futc).transfer(spender , (dh * three)/100);
        }
        IERC20(usdt).transferFrom(msg.sender, address(this), _cx);
        IERC20(futc).transfer(msg.sender, dh);
        IERC20(usdt).transfer(ysz, _cx);
    }

    function calculation(uint _cx,uint _bili) external view returns (uint cals){
        uint cal = _cx * ratio * 100 * _bili / 100;
        return cal;
    }
    function gettui (address tui) external view returns (address cals){
        return  Deployed(fff).tui(tui) == address(0) ? tui2[tui] : Deployed(fff).tui(tui);
    }

    function gettuinum (address tui) external view returns (uint256 cals){
        return Deployed(fff).tuinum(tui) + tuinum2[tui];
    }
    function setusdt (address _usdt) external onlyOwner {
        usdt = _usdt;
    }
    function setysz (address _ysz) external onlyOwner { 
        ysz = _ysz;
    }

    function setfutc (address _futc) external onlyOwner {
        futc = _futc;
    }
    function setfff (address _fff) external onlyOwner { 
        fff = _fff;
    }

    function withdraw () external onlyOwner {
        uint balance = IERC20(usdt).balanceOf(address(this));
        IERC20(usdt).transfer(msg.sender, balance);
    }
 
    function setOwnership(address newOwner) external onlyOwner {
        require(newOwner != address(0), "Ownable: new owner is the zero address");
        _transferOwnership(newOwner);
    }


}