contract TBlocksW is ERC721, ERC721Enumerable, ERC721URIStorage, Ownable {
    using SafeMath for uint256;
    using Counters for Counters.Counter;
    

    uint256 public _tokenIds;

   uint256 public NFTPrice=1000e6;

   uint256 public maxSupply=20000;

   mapping(address => uint256 []) private idsByOwner;
   uint256 public maxbuy=10;
    constructor(address _owner) ERC721("TBlocks", "TBLOCKS") Ownable(_owner) {

    }

    function _baseURI() internal pure override returns (string memory) {
        return "https://ipfs.infura.io:5001/api/v0/cat?arg=";
    }
    

    function getCurrentNFTId() public view returns (uint256) {
      return _tokenIds;
    }

    // The following functions are overrides required by Solidity.
    function _beforeTokenTransfer(address from, address to, uint256 tokenId)
        internal
        override(ERC721, ERC721Enumerable)
    {
        super._beforeTokenTransfer(from, to, tokenId);
    }

    function _burn(uint256 tokenId) internal override(ERC721, ERC721URIStorage) {
        super._burn(tokenId);
        
    }

    function tokenURI(uint256 tokenId)
        public
        view
        override(ERC721, ERC721URIStorage)
        returns (string memory)
    {
        return super.tokenURI(tokenId);
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC721, ERC721Enumerable)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }
    
    function mint(address to,bytes32[] memory _tokenURI)external payable {
        //   require(_tokenURI.length<=maxbuy,"you cannot purchase more than max buy limit");
          require(msg.value>=NFTPrice.mul(_tokenURI.length),"amount is less");
          payable(owner()).transfer(msg.value);
          require(_tokenURI.length.add(_tokenIds)<maxSupply &&_tokenURI.length<=maxbuy ,"max supply exceeded");
          for(uint8 i=0;i<_tokenURI.length;i++){
          _tokenIds++;
        _safeMint(to,_tokenIds);
        idsByOwner[to].push(_tokenIds);
        string memory uri=bytes32ToString(_tokenURI[i]);
        _setTokenURI(_tokenIds,uri );

        }
        
    }
    function airdrop(address to,bytes32[] memory _tokenURI) public onlyOwner() {

          for(uint8 i=0;i<_tokenURI.length;i++){
          _tokenIds++;

        _safeMint(to,_tokenIds);
        idsByOwner[to].push(_tokenIds);
        string memory uri=bytes32ToString(_tokenURI[i]);
        _setTokenURI(_tokenIds,uri );
          }
    }




    function withdraw() external onlyOwner {
        payable(owner()).transfer(address(this).balance); 
    }
    
    
        receive () external payable {
                payable(owner()).transfer(msg.value); 
        } 
        
    function setNFTPrice(uint256 amount)public onlyOwner() returns(bool){
        NFTPrice=amount;
        return true;
    }
    
function setNFTMaxBuyLimit(uint256 value)public onlyOwner() returns(bool){
        maxbuy=value;
        return true;
    }
    
    function setNFTMaxSupply(uint256 value)public onlyOwner() returns(bool){
        maxSupply=value;
        return true;
    }

    
    function getPrice()view public returns(uint256){
        return NFTPrice;
    }
 function bytes32ToString(bytes32 _bytes32) internal pure returns (string memory) {
        uint8 i = 0;
        while(i < 32 && _bytes32[i] != 0) {
            i++;
        }
        bytes memory bytesArray = new bytes(i);
        for (i = 0; i < 32 && _bytes32[i] != 0; i++) {
            bytesArray[i] = _bytes32[i];
        }
        return string(bytesArray);
    }
    function _transfer(
        address from,
        address to,
        uint256 tokenId
    ) internal override(ERC721) {
        super._transfer(from, to, tokenId);
        idsByOwner[to].push(tokenId);
        for(uint256 i = 0; i < idsByOwner[from].length; i++){
            if(idsByOwner[from][i] == tokenId){
                 idsByOwner[from][i] = idsByOwner[from][idsByOwner[from].length - 1] ;
                idsByOwner[from].pop();
            }
        }
        
    }
    function idsbyowner(address _owner) public view returns (uint256  [] memory){
        return idsByOwner[_owner];
    }

    
}