contract LBTToken is TRC20, Ownable {
    uint256 private _cap = 1e26;
    uint256 private initialSupply = 0;

    constructor() public TRC20('Liberti.Finance', 'LBT') {
        _mint(msg.sender, initialSupply);
    }

    function mint(address _to, uint256 _amount)
        public
        onlyOwner
        returns (bool)
    {
        require(
            totalSupply().add(_amount) <= _cap,
            'TRC20Capped: cap exceeded'
        );
        _mint(_to, _amount);
        return true;
    }

    /**
     * @dev Returns the cap on the token's total supply.
     */
    function cap() public view returns (uint256) {
        return _cap;
    }

    /**
     * @dev Destroys `amount` tokens from the caller.
     *
     * See {TRC20-_burn}.
     */
    function burn(uint256 amount) public returns (bool) {
        _burn(_msgSender(), amount);
        return true;
    }

    /**
     * @dev Destroys `amount` tokens from `account`, deducting from the caller's
     * allowance.
     *
     * See {TRC20-_burn} and {TRC20-allowance}.
     *
     * Requirements:
     *
     * - the caller must have allowance for ``accounts``'s tokens of at least
     * `amount`.
     */
    function burnFrom(address account, uint256 amount) public returns (bool) {
        uint256 decreasedAllowance = allowance(account, _msgSender()).sub(
            amount,
            'TRC20: burn amount exceeds allowance'
        );

        _approve(account, _msgSender(), decreasedAllowance);
        _burn(account, amount);
        return true;
    }
}